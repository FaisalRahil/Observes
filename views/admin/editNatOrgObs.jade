extends ../layout
block content
  header.navbar.navbar-red.navbar-fixed-top.bs-docs-nav(role='banner')
    .navbar-header.pull-left
      button.navbar-toggle(type='button', data-toggle='collapse', data-target='.bs-navbar-collapse')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      h3.marginLeft.text-muted.text-left 
        a(href="/")
          img(src="/images/hnec_logo.png")
    nav.collapse.navbar-collapse.bs-navbar-collapse(role='navigation')
      ul.fontSizeNav.nav.navbar-nav
        li.dropdown
          a.dropdown-toggle(href='#', data-toggle='dropdown')
            | المستخدمين 
            b.caret
          ul.dropdown-menu
            li
              a(href='#') Action
            li
              a(href='#') بيانات المستخدم
            li
              a(href='#') خروج
        li
          a(href='#') التقارير
        li
          a(href='#') المراقبين
        li
          a(href='#') الـمنظمات

  .row
    .col-md-8.col-md-offset-2
      .form-group
        input#discount_flag(type='checkbox', name='discount_flag')
      table#OrgObs.table.table-bordered.table-striped(style='clear: both')
        tbody
          tr
            td(width='35%') الرقم الوطني / جواز سفر
            td(width='65%')
              a#pass_nid(href='#', data-type='text', data-pk='#{OrgObs[0].id_ob}', data-title='الرقم الوطني / جواز سفر' ,data-value='#{OrgObs[0].pass_nid}') #{OrgObs[0].pass_nid}
          tr
            td(width='35%') الاسم
            td(width='65%')
              a#name_obs(href='#', data-type='text', data-pk='#{OrgObs[0].id_ob}', data-title='الاسم' ,data-value='#{OrgObs[0].name_obs}') #{OrgObs[0].name_obs}
          tr
            td(width='35%') رقم الهاتف
            td(width='65%')
              a#phone(href='#', data-type='text', data-pk='#{OrgObs[0].id_ob}', data-title='رقم الهاتف' ,data-value='#{OrgObs[0].phone}') #{OrgObs[0].phone}
          tr
            td(width='35%') البريد الاكتروني
            td(width='65%')
              a#email(href='#', data-type='text', data-pk='#{OrgObs[0].id_ob}', data-title='البريد الاكتروني', data-value='#{OrgObs[0].email}') #{OrgObs[0].email}

  
  script(src='/js/jquery-1.11.1.min.js')
  script(src='/js/bootstrap.min.js')
  script(src='/js/bootstrap-table.min.js')
  script(src="/js/bootstrap-editable.min.js")
  script(src="/js/jquery.validate.min.js")
  script(src='/js/bootstrap-switch.min.js')
  script(src="/js/admin/editNatOrgObs.js")
  script(src='/js/admin/backToTop.js')
  