extends ../layout
block content
  header.navbar.navbar-blue.navbar-fixed-top.bs-docs-nav(role='banner')
    .navbar-header.pull-left
      button.navbar-toggle(type='button', data-toggle='collapse', data-target='.bs-navbar-collapse')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      h3.marginLeft.text-muted.text-left 
        a(href="/")
          img(src="/images/hnec_logo.png")
    nav.collapse.navbar-collapse.bs-navbar-collapse(role='navigation')
      ul.fontSizeNav.nav.navbar-nav
        li.dropdown
          a.dropdown-toggle(href='#', data-toggle='dropdown')
            | المستخدمين 
            b.caret
          ul.dropdown-menu
            li
              a(href='#') Action
            li
              a(href='#') بيانات المستخدم
            li
              a(href='/logout') خروج
        li
          a(href='/reports') التقارير
        li
          a(href='/admin/obs') المراقبين
        li
          a(href='/admin/org') الـمنظمات


  .row
    .col-md-10.col-md-offset-1
      .panel.panel-info
        .panel-heading
          h3.panel-title
            | نقل المراقبين
        br

        .row
          form#formTrn(role="form",name='formTrn')
            .col-md-4.col-md-offset-1.dual-list
              .row
                .col-md-10
                  label.h4 من
                  select#org_old.form-control.selectpicker.show-tick(name="id_org_old", data-live-search="true", data-size=5)
                    option(value="", style='color:grey; display:none;') الرجاء اختيار 
                    for org in orgs
                      option(value="#{org.id_org}") #{org.name_org}
                .col-md-2

            .col-md-4.col-md-offset-1.dual-list.list-left
              .row
                .col-md-10
                  label.h4 الى
                  select#org_new.form-control.selectpicker.show-tick(name="id_org_new", data-live-search="true", data-size=5)
                    option(value="", style='color:grey; display:none;') الرجاء اختيار 
                    for org in orgs
                      option(value="#{org.id_org}") #{org.name_org}
                .col-md-2
            .col-xs-2.col-md-2
              button#btnSubmit.btn.btn-warning.btn-block(type="button") تسجيــــل         


  script(src="/js/jquery-1.11.1.min.js")
  script(src="/js/bootstrap.min.js")
  script(src="/js/bootstrap-select.min.js")
  script(src="/js/admin/bootstrap-notify.min.js")
  script(src="/js/helpers.js")
  script(src="/js/admin/moveOrg.js")